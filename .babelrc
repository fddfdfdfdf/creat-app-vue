{
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "not ie <  9"
            ]
          },
          "modules": false,
          "debug": false,
          "useBuiltIns": "usage"
        }
      ]
  ]
}
